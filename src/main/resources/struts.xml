<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<!-- Author: David -->
 
<struts>
 
    <constant name="struts.devMode" value="true" />
    <constant name="struts.custom.i18n.resources" value="global" />
    
    <package name="mibiblio" extends="struts-default">
        <!--
        Esto hace que cualquier acciÃ³n utilice Listado_Cliente como respuesta
        <default-action-ref name="Listado_Cliente"/>
        -->
        <action name="inventory" method="listado" class="dgg.ucav.dao.ObraAccion">
            <result>/WEB-INF/jsp/inventory.jsp</result>
        </action>
        
        <action name="index" method="novedades" class="dgg.ucav.dao.NovedadesAccion">
            <result>/WEB-INF/jsp/index.jsp</result>
        </action>
        
        <action name="locale" 
                class="dgg.ucav.mibiblio.locale.action.Locale"
                method="execute">
            <result name="success">/WEB-INF/jsp/index.jsp</result>
        </action>
   
    </package>
 
</struts>